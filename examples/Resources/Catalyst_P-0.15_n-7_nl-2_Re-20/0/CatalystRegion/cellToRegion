FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      cellToRegion;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    cylExtSurf
    {
        type            zeroGradient;
    }
    Catalyst_to_Channels
    {
        type            zeroGradient;
    }
    inletCatalyst_to_inletCatalystFluidWall
    {
        type            zeroGradient;
    }
    outletCatalyst_to_outletCatalystFluidWall
    {
        type            zeroGradient;
    }
}


